#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.15.16_sass-embedded@1.87.0__vue@3.5.13/node_modules/vuepress/bin/node_modules:/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.15.16_sass-embedded@1.87.0__vue@3.5.13/node_modules/vuepress/node_modules:/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.15.16_sass-embedded@1.87.0__vue@3.5.13/node_modules:/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.15.16_sass-embedded@1.87.0__vue@3.5.13/node_modules/vuepress/bin/node_modules:/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.15.16_sass-embedded@1.87.0__vue@3.5.13/node_modules/vuepress/node_modules:/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.15.16_sass-embedded@1.87.0__vue@3.5.13/node_modules:/Users/mark/Documents/vuepress_learn_project/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-webpack.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-webpack.js" "$@"
fi
